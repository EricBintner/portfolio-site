



<!DOCTYPE html>
<html lang="en" >

<head>
  <meta charset="UTF-8">
  <title>New Site</title>

  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
  <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js" type="text/javascript"></script>
	  
	  <link href="https://fonts.googleapis.com/css?family=Dosis:400,600,800|Monoton" rel="stylesheet">  
      
      <link rel="stylesheet" type="text/css" href="css/main.css">
      <link rel="stylesheet" type="text/css" href="css/nav.css">
      <link rel="stylesheet" type="text/css" href="css/isotope.css">
      <link rel="stylesheet" type="text/css" href="css/level_0.css">

</head>

<body>
	

    <div id="environment">
      <div id="content">

<!-- 	// FOR EACH SECTION
		//      0: Intro
		// STOP 1: Skills
		// STOP 2: Interactive
		// STOP 3: Motion
		// STOP 4: Art
		// STOP 5: Credits 
-->

        <section id="first"> 

        	{{> SVG-background_first}}

        	<div class="perspectiveContainer">
        		{{!> type-skills}}
        		<div id="" class="button-group">  
        		  
				</div>
        	</div>
        
        	<div class="place3D stop1">
        		{{> SVG-background_first}}
        		<!--<div id="wp1" class="waypoint-content"></div>-->
        	</div>
        	<div class="place3D stop2">
        		{{> SVG-background_first}}
        	</div>
        	<div class="place3D stop3">
        		{{> SVG-background_first}}
        	</div>
        	<div class="place3D stop4">
        		{{> SVG-background_first}}
        	</div>
        	<div class="place3D stop5">
        		{{> SVG-background_first}}
        	</div>
        </section>



        <section id="second">
        	{{> SVG-background_second}}
        	<div class="place3D stop1">
        		<div class="holder skillHolder">

					<div class="grid skills">
					</div>
        			<!--{!{> content-skills}}-->
        		</div>
        		{{> SVG-background_second}}
        	</div>
        	<div class="place3D stop2">
        		<div class="holder">
        			{{> content-interactive}}
        		</div>
        		{{> SVG-background_second}}
        	</div>
        	<div class="place3D stop3">
        		{{> SVG-background_second}}
        	</div>
        	<div class="place3D stop4">
        		 {{> SVG-background_second}}
        	</div>
        	<div class="place3D stop5">
        		 {{> SVG-background_second}}
        	</div>
        </section>



        <section id="third"> 
        	<div class="perspectiveContainer">
        		{{> type-intro}}
        		<div class="perspectiveContainer">
        			{{!> type-skills}}
        			<div id="" class="button-group">  

					</div>
        		</div>
        	</div>
        	{{> SVG-background_third}}
        	<div class="place3D stop1">
        		<div class="perspectiveContainer">
        			<div class="typeFix">
	        			{{> type-skills}}
	        		</div>
					<div id="filters" class="button-group skills">  
					  <button class="button notButton" data-filter="*"><p>FILTERS</p></button>
					  <button class="button" data-filter=".Presentation-Development">
					  	<span>Presentation</span>
					  	<p><span>DEV / DESIGN</span></p>
					  </button>
					  <button class="button" data-filter=".Structural-Development">
					  	<span>Structural</span>
					  	<p><span>DEVELOPMENT</span></p>
					  </button>
					  <button class="button" data-filter=".Software">
					  	<span>Software</span>
					  	<p><span>TOOLS</span></p>
					  </button>
					  <button class="button" data-filter=".UX-Design">
					  	<span>UX</span>
					  	<p><span>DESIGN</span></p>
					  </button>
					</div>

					<div id="sorts" class="button-group skills">  
					  <button class="button" data-sort-by="mastery">mastery</button>
					  <button class="button" data-sort-by="experience">experience</button>
					  <button class="button" data-sort-by="love">love</button>
					</div>
        		</div>
        		{{> SVG-background_third}}
        	</div>
        	<div class="place3D stop2">
        		<div class="perspectiveContainer">
        			<div class="typeFix">
        				{{> type-interactive}}
        			</div>
        			<div id="filters" class="button-group interactive">  
        			  <button class="button is-checked" data-filter="*">show all</button>
					  <button class="button" data-filter=".dataset">dataset</button>
					</div>
        		</div>
        		{{> SVG-background_third}}
        	</div>
        	<div class="place3D stop3">
        		<div class="perspectiveContainer">
        			<div class="typeFix">
	        			{{> type-motion}}
	        		</div>
        			<div id="" class="button-group">  
        			  <!--  - - - - - - - - -  -->
					</div>
        		</div>
        		{{> SVG-background_third}}
        	</div>
        	<div class="place3D stop4">
        		<div class="perspectiveContainer">
        			<div class="typeFix">
	        			{{> type-art}}
	        		</div>
        			<div id="" class="button-group">  
        			  <!--  - - - - - - - - -  -->
					</div>
        		</div>
        		{{> SVG-background_third}}
        	</div>
        	<div class="place3D stop5">
        		{{> SVG-background_third}}
        	</div>
        </section>



        <section id="fourth">
        	{{> SVG-background_fourth}}
        	<div class="place3D stop1">
        		{{> SVG-background_fourth}}
        	</div>
        	<div class="place3D stop2">
        		{{> SVG-background_fourth}}
        	</div>
        	<div class="place3D stop3">
        		{{> SVG-background_fourth}}
        	</div>
        	<div class="place3D stop4">
        		{{> SVG-background_fourth}}
        	</div>
        	<div class="place3D stop5">
        		{{> SVG-background_fourth}}
        	</div>
        </section>



      </div>
    </div>

    <div id="scroll-proxy"></div>

    <nav>
	  <!-- <div class="navPos">
		<a class="level-G" data-level="0" data-yaxis="0"><span>Earth</span></a>
		<a class="level-1" data-level="1" data-yaxis="16"><span>Troposphere</span></a>
		<a class="level-2" data-level="2" data-yaxis="32"><span>Stratosphere</span></a>
		<a class="level-3" data-level="3" data-yaxis="48"><span>Mesosphere</span></a>
		<a class="level-4" data-level="4" data-yaxis="64"><span>Thermosphere</span></a>
		<a class="level-5" data-level="5" data-yaxis="80"><span>Exosphere</span></a>
	  </div> -->

	  <div class="navPos">
	  	{{> metro}}
		<a class="level-G" data-level="0" data-yaxis="0"><span>Intro</span><div class="subStop"></div></a>
		<a class="level-1" data-level="1" data-yaxis="16"><span>Skills</span><div class="subStop"></div></a>
		<a class="level-2" data-level="2" data-yaxis="32"><span>Interactive</span><div class="subStop"></div></a>
		<a class="level-3" data-level="3" data-yaxis="48"><span>Motion</span><div class="subStop"></div></a>
		<a class="level-4" data-level="4" data-yaxis="64"><span>Art</span><div class="subStop"></div></a>
		<a class="level-5" data-level="5" data-yaxis="80"><span>Credits</span><div class="subStop"></div></a>
	  </div>
	</nav>

